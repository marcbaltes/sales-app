if(!window.CategoryList){
    window.CategoryList = {};
}
//Add Category, specify a category name and a value for it in the below line of code.
//This will dynamically create an entry to the category object.
var mapOfCategory = Object.freeze(
    {
        ELIGIBILITY: 'Eligibility Status'
    });
var mapOfActions = Object.freeze(
    {
        RE_VERIFY: 'Re-Verify',
        VERIFY: 'Verify'
    });
//The below function creates te Category and Action list of values.
(function(categoryList) {

    //The below configuration make the values immutable and assigns the
    //above specified values.
    categoryList.CATEGORY = mapOfCategory;
    categoryList.ACTIONS = mapOfActions;
    Object.freeze(categoryList);
}(CategoryList));